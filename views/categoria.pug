extends ./layouts/app

block styles
    link(rel="stylesheet" href="https://unpkg.com/leaflet@1.8.0/dist/leaflet.css")

block contenido 
    

    section(class="py-5")
        h2(class="pt-10 text-4xl font-extrabold text-center")= pagina

        if propiedades.length
            div(class="grid grid-cols-1 gap-5 mt-14 lg:grid-cols-3")
                each propiedad in propiedades
                    include ./templates/card
        else 
            p(class="py-10 text-xl text-center text-gray-800") No hay resultados

